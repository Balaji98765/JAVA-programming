import java.util.*;
public class Main
{
	public static void main(String[] args) {
		Scanner s=new Scanner(System.in);int m=0;
		int a=s.nextInt();
		int b=s.nextInt();
	for(int i=0;i<a;i++){
	      if(i%2==0){m=i*a+1;
	  for(int j=0;j<b;j++){
	    System.out.print((m++)+" ");
	  }
	 System.out.println();}
	 else{m=(i+1)*a;
	   for(int j=b-1;j>=0;j--){
	        System.out.print((m--)+" ");
	   }	 System.out.println();
	   
			
	}}
}}
